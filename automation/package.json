{
  "name": "automation",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "book": "playwright test tests/book_production.spec.ts --workers=1",
    "book:live": "cross-env FS_DRY_RUN=false playwright test tests/book_production.spec.ts --workers=1",
    "normal": "cross-env FS_MODE=normal npx playwright test tests/book_production.spec.ts --workers=1 --reporter=list",
    "snipe": "cross-env FS_MODE=snipe npx playwright test tests/book_production.spec.ts --workers=1 --trace on",
    "release": "cross-env FS_MODE=snipe node scripts/wait-and-snipe.js",
    "codegen": "playwright codegen"
  },
