# Fairway Sniper agent local environment
# Copy to .env and fill in your values. Never commit .env.

# BRS member login
BRS_USERNAME=your_brs_gui
BRS_PASSWORD=your_brs_password

# Firebase service account (optional for local-only runs you can omit)
FIREBASE_PROJECT_ID=
FIREBASE_CLIENT_EMAIL=
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"

# FCM server key for push notifications
FCM_SERVER_KEY=

# Club login URL and timezone
CLUB_LOGIN_URL=https://members.brsgolf.com/galgorm/login
TZ_LONDON=Europe/London

# Set to true to actually run main booking loop when invoking node index.js
AGENT_RUN_MAIN=false
